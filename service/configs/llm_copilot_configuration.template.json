{
  "primary_model":{
    "ai_provider":"azure_openai",
    "azure_openai_text_completion_endpoint":<ENDPOINT_URL>,
    "azure_openai_text_completion_token":<KEY>,
    "azure_openai_text_completion_deployment_name":<DEPLOYMENT_NAME>,
    "azure_openai_embedding_endpoint":<EMBEDDING_ENDPOINT_URL>,
    "azure_openai_embedding_token":<EMBEDDING_KEY>,
    "azure_openai_embedding_deployment_name":<EMBEDDING_DEPLOYMENT_NAME>
  },
  "secondary_model":{
    "ai_provider":"openai",
    "openai_text_completion_token":<KEY>,
    "openai_text_completion_model_name":<DEPLOYMENT_NAME>
  }
}

// or use the following embedding properties if you will use openai as the primary model
"openai_text_completion_token":<EMBEDDING_KEY>,
"openai_embedding_model_name":<EMBEDDING_DEPLOYMENT_NAME>